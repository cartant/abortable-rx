{
  "compilerOptions": {
    "strict": true,
    "target": "es2018",
    "module": "es2015",
    "moduleResolution": "node",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "lib",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noUnusedParameters": false,
    "lib": ["es2017", "dom", "dom.iterable"]
  }
}
